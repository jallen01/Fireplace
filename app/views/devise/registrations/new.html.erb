<div class="row no_left_margin devise_form">
<div class="span4 offset4">

<h2>Sign up</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label :first_name, "First Name:" %>
  <%= f.text_field :first_name, :autofocus => true %></div>

  <div><%= f.label :last_name, "Last Name:" %>
  <%= f.text_field :last_name %></div>

  <div><%= f.label :email, "Email:" %>
  <%= f.email_field :email %></div>

  <div><%= f.label :password, "Password:" %>
  <%= f.password_field :password %></div>

  <div><%= f.label :password_confirmation, "Password Confirmation:" %>
  <%= f.password_field :password_confirmation %></div>

  <div><%= f.submit "Sign up" %></div>
<% end %>

<%= render "devise/shared/links" %>

</div>
</div>