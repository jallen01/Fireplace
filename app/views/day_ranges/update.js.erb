<% unless @day_range.errors.any? %>
    $("#day-ranges-list .list-group").html("<%= j render "day_ranges/list" %>");
    $("<%= j "#edit-day-range-#{@day_range.id}-modal" %>").modal("hide");
<% end %>

$("<%= j "#edit-day-range-#{@day_range.id}-modal .modal-body" %>").html("<%= j render "day_ranges/form", day_range: @day_range %>");

