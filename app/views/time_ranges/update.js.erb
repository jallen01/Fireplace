$('#edit-time-range-#{@time_range.id}-modal .modal-body').html('<%= j render "time_ranges/form", time_range: @time_range %>');

<% unless @time_range.errors.any? %>
    $('#edit-time-range-#{@time_range.id}-modal').modal('hide');
    $("#time-ranges-list").html('<%= j render "time_ranges/list" %>');
<% end %>