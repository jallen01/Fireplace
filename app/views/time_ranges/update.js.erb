<% unless @time_range.errors.any? %>
	$("#time-ranges-list .list-group").html("<%= j render "time_ranges/list" %>");
	$("<%= j "#edit-time-range-#{@time_range.id}-modal" %>").modal('hide');	
<% end %>

$("<%= j "#edit-time-range-#{@time_range.id}-modal .modal-body" %>").html("<%= j render "time_ranges/form", time_range: @time_range %>");

